<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="IntroSceneParent" prefHeight="920.0" prefWidth="920.0" stylesheets="@IntroSceneStyle.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.IntroSceneControl">
   <children>
      <Button fx:id="game_button" alignment="CENTER" contentDisplay="CENTER" layoutX="400.0" layoutY="442.0" mnemonicParsing="false" onAction="#game_click" prefHeight="37.0" prefWidth="131.0" text="PLAY">
         <font>
            <Font size="24.0" />
         </font></Button>
      <ImageView cache="true" fitHeight="242.0" fitWidth="610.0" layoutX="151.0" layoutY="158.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/Snake_VS_Snake_logo.png" />
         </image>
      </ImageView>
      <Button fx:id="EXIT_Button" layoutX="401.0" layoutY="603.0" mnemonicParsing="false" onAction="#EXIT" prefHeight="37.0" prefWidth="131.0" text="EXIT">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="Ranking_button" layoutX="396.0" layoutY="520.0" mnemonicParsing="false" onAction="#Highscores_click" prefHeight="51.0" prefWidth="140.0" text="RANKING">
         <font>
            <Font size="24.0" />
         </font></Button>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
